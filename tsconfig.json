{
  "compilerOptions": {
    "strict": true,
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [ "es2022" ],
    "rootDir": "src",
    "outDir": "build",
    "skipLibCheck": true,
    "jsx": "react",
    "typeRoots": ["node_modules/@fastly", "./src/declaration.d.ts"],
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules"
  ]
}
